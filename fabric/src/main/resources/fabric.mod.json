{
  "schemaVersion": 1,
  "id": "veggycraft",
  "version": "${version}",
  "name": "VeggyCraft",
  "description": "A mod that adds vegan items and recipes to usual vanilla Minecraft items that aren't vegan themselves!",
  "authors": [
    "Klayil"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "CC0-1.0",
  "icon": "assets/veggycraft/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.klayil.veggycraft.fabric.VeggyCraftFabric"
    ],
    "client": [
      "net.klayil.veggycraft.fabric.client.VeggyCraftFabricClient"
    ]
  },
  "mixins": [
    "veggycraft.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.18.1",
    "minecraft": "~1.21.10",
    "java": ">=21",
    "architectury": ">=18.0.6",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
